{
  "name": "minimax-supabase-integration",
  "version": "1.0.0",
  "description": "Minimax AI agent integration with Supabase authentication and edge functions",
  "type": "module",
  "main": "client.js",
  "scripts": {
    "dev": "node --watch example.js",
    "build": "echo 'Static site - no build needed'",
    "test": "node test.js",
    "logs": "supabase functions logs minmax-agent-auth",
    "setup": "npm run install-cli && npm run login",
    "install-cli": "npm install -g supabase",
    "login": "supabase login",
    "link": "supabase link",
    "deploy": "supabase functions deploy minmax-agent-auth",
    "secrets:set": "echo 'Run: supabase secrets set MINIMAX_API_KEY=your_key_here'",
    "local:start": "supabase start",
    "local:stop": "supabase stop"
  },
  "keywords": [
    "minimax",
    "supabase",
    "ai",
    "agent",
    "edge-functions",
    "authentication"
  ],
  "author": "Your Name",
