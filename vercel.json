{
  "version": 2,
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": "public",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/admin-login.html",
      "dest": "/admin-login.html"
    },
    {
      "src": "/admin-superpage.html",
      "dest": "/admin-superpage.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
